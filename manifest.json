{
  "manifest_version": 3,
  "name": "Discord Joiner",
  "description": "A simple extension to help you enter in discord invite links as fast as possible",
  "version": "0.1",
  "action": {
    "default_popup": "popup.html"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+Space",
        "windows": "Ctrl+Shift+Space",
        "mac": "Command+Shift+Space",
        "chromeos": "Ctrl+Shift+Space",
        "linux": "Ctrl+Shift+Space"
      }
    }
  }
}